    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "electron": "^2.0.2",
    "electron-builder": "^19.53.6",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
